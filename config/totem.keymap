            //                                     ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛   ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛

            bindings = <
              &kp DE_Q       &kp DE_W       &kp DE_E        &kp DE_R        &kp DE_T         &kp DE_Z      &kp DE_U        &kp DE_I        &kp DE_O       &kp DE_P
              &mt LGUI DE_A  &mt LALT DE_S  &mt LCTRL DE_D  &mt LSHFT DE_F  &kp DE_G         &kp DE_H      &mt RSHFT DE_J  &mt RCTRL DE_K  &mt RALT DE_L  &mt RGUI O
&kp DE_CARET  &kp DE_Y       &kp DE_X       &kp DE_C        &kp DE_V        &kp DE_B         &kp DE_N      &kp DE_M        &kp DE_COMMA    &kp DE_PERIOD  &kp DE_MINUS  &kp DE_HASH
